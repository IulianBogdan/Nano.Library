  "Data": {
    "BatchSize": 25,
    "BulkBatchSize": 500,
    "BulkBatchDelay": 1000,
    "QueryRetryCount": 0,
    "QueryIncludeDepth": 4,
    "UseAudit": true,
    "UseMemoryCache": true,
