  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\**\*.cshtml" /> 
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Views\_ViewImports.cshtml" />
    <EmbeddedResource Remove="Views\_ViewStart.cshtml" />
  </ItemGroup> 
</Project>
